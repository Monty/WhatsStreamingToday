{
  "_id": "britbox_seasons",
  "startUrl": [
    "https://www.britbox.com/us/movie/70_Glorious_Years_13550",
    "https://www.britbox.com/us/movie/A_Christmas_Carol_15128",
    "https://www.britbox.com/us/show/A_Bit_of_Fry_and_Laurie_6393",
    "https://www.britbox.com/us/show/A_History_of_Ancient_Britain_6385",
    "https://www.britbox.com/us/show/New_Blood_9275",
    "https://www.britbox.com/us/show/The_Jury_9565",
    "https://www.britbox.com/us/show/Till_Death_Us_Do_Part_15810",
    "https://www.britbox.com/us/show/Vera_13500"
  ],
  "selectors": [
    {
      "id": "ProgramInfo",
      "type": "SelectorElement",
      "selector": "div.brand-hero-info",
      "parentSelectors": ["_root"],
      "multiple": false,
      "delay": ""
    },
    {
      "id": "ProgramTitle",
      "type": "SelectorText",
      "selector": "h1.brand-hero-info__title",
      "parentSelectors": ["ProgramInfo"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "SeasonCarousel",
      "type": "SelectorElement",
      "selector": "div.carousel__content",
      "parentSelectors": ["_root"],
      "multiple": false,
      "delay": ""
    },
    {
      "id": "Season",
      "type": "SelectorElement",
      "selector": "a.brand-season-item",
      "parentSelectors": ["SeasonCarousel"],
      "multiple": true,
      "delay": ""
    },
    {
      "id": "URL",
      "type": "SelectorElementAttribute",
      "selector": "a.brand-season-item",
      "parentSelectors": ["Season"],
      "multiple": false,
      "extractAttribute": "href",
      "delay": ""
    },
    {
      "id": "SeasonTitle",
      "type": "SelectorText",
      "selector": "h2.program-item__program-title",
      "parentSelectors": ["Season"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "SeasonNumber",
      "type": "SelectorText",
      "selector": "h2.program-item__program-title",
      "parentSelectors": ["Season"],
      "multiple": false,
      "regex": "^(Season|Series) \\d+$",
      "delay": ""
    },
    {
      "id": "Year",
      "type": "SelectorText",
      "selector": "p.season-metadata",
      "parentSelectors": ["Season"],
      "multiple": false,
      "regex": "(\\d+)",
      "delay": ""
    },
    {
      "id": "NumEpisodes",
      "type": "SelectorText",
      "selector": "p.season-metadata span",
      "parentSelectors": ["Season"],
      "multiple": false,
      "regex": "(\\d+)",
      "delay": ""
    },
    {
      "id": "SeasonDescription",
      "type": "SelectorText",
      "selector": "p.brand-hero-info__description",
      "parentSelectors": ["ProgramInfo"],
      "multiple": false,
      "regex": "",
      "delay": ""
    }
  ]
}
