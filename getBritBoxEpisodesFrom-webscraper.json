{
  "_id": "single-multi_season-movie",
  "startUrl": ["https://www.britbox.com/us/programmes"],
  "selectors": [
      {
      "id": "PgmURL",
      "type": "SelectorLink",
      "selector": "div.program-item a[href*='/us/'].program-item__block",
      "parentSelectors": ["_root"],
      "multiple": true,
      "delay": "8000"
    },
    {
      "id": "SsnURL",
      "type": "SelectorLink",
      "selector": "a[href*='/us/season/'].brand-season-item",
      "parentSelectors": ["PgmURL"],
      "multiple": true,
      "delay": ""
    },
    {
      "id": "SsnCarousel",
      "type": "SelectorElement",
      "selector": "div.episodes-carousel",
      "parentSelectors": ["SsnURL"],
      "multiple": false,
      "delay": ""
    },
    {
      "id": "EpiCarousel",
      "type": "SelectorElement",
      "selector": "div.carousel__content",
      "parentSelectors": ["PgmURL"],
      "multiple": false,
      "delay": ""
    },
    {
      "id": "Program",
      "type": "SelectorElement",
      "selector": "div.program-item",
      "parentSelectors": ["SsnCarousel", "EpiCarousel"],
      "multiple": true,
      "delay": ""
    },
    {
      "id": "URL",
      "type": "SelectorElementAttribute",
      "selector": "a[href*='/us/'].program-item__block",
      "parentSelectors": ["Program"],
      "multiple": false,
      "extractAttribute": "href",
      "delay": ""
    },
    {
      "id": "Program_Title",
      "type": "SelectorText",
      "selector": "h3.program-item__program-title",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Episode_Title",
      "type": "SelectorText",
      "selector": "p.program-item__program-subtitle",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Year",
      "type": "SelectorText",
      "selector": "span.programme-metadata__year",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Duration",
      "type": "SelectorText",
      "selector": "span.programme-metadata__duration",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Rating",
      "type": "SelectorText",
      "selector": "span.programme-metadata__classification",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Description",
      "type": "SelectorText",
      "selector": "p.program-item__program-description",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    }
  ]
}
