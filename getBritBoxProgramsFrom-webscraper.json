{
  "_id": "BritBoxPrograms",
  "startUrl": ["https://www.britbox.com/us/programmes"],
  "selectors": [
    {
      "id": "Program",
      "type": "SelectorElementScroll",
      "selector": "div.program-item",
      "parentSelectors": ["_root"],
      "multiple": true,
      "delay": "8000"
    },
    {
      "id": "URL",
      "type": "SelectorElementAttribute",
      "selector": "a[href*='/us/'].program-item__block",
      "parentSelectors": ["Program"],
      "multiple": false,
      "extractAttribute": "href",
      "delay": ""
    },
    {
      "id": "Program_Title",
      "type": "SelectorText",
      "selector": "h3.program-item__program-title",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Sn_Years",
      "type": "SelectorText",
      "selector": "span.programme-season-metadata__year",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Seasons",
      "type": "SelectorText",
      "selector": "span.programme-season-metadata__count",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "\\d+ Seasons?",
      "delay": ""
    },
    {
      "id": "Mv_Year",
      "type": "SelectorText",
      "selector": "span.programme-metadata__year",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Duration",
      "type": "SelectorText",
      "selector": "span.programme-metadata__duration",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Rating",
      "type": "SelectorText",
      "selector": "span.programme-metadata__classification",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    },
    {
      "id": "Description",
      "type": "SelectorText",
      "selector": "p.program-item__program-description",
      "parentSelectors": ["Program"],
      "multiple": false,
      "regex": "",
      "delay": ""
    }
  ]
}
